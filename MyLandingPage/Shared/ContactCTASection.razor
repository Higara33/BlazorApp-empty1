@inject NavigationManager Nav
@inject IStringLocalizer<Shared.ContactCTASection> L

<section id="contact-cta" class="py-5 text-white">
    <div class="container text-center">
        <h2 class="mb-4">@L["CTA_Title"]</h2>
        <p class="lead mb-4">@L["CTA_Subtitle"]</p>
        <button class="btn btn-primary btn-lg" @onclick='() => Nav.NavigateTo("survey/sail")'>
            @L["CTA_Button"]
        </button>
    </div>
</section>